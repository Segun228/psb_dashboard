          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n    TO_DATE(CONCAT(md.year, '-', md.month, '-01'), 'YYYY-MM-DD') as time,\n    md.metric,\n    ml.month_name,\n    MAX(CASE WHEN md.metric_type = 'Факт' THEN md.value END) as fact,\n    MAX(CASE WHEN md.metric_type = 'План' THEN md.value END) as plan\nFROM metrics_data md\nJOIN months_lookup ml ON md.month = ml.month\nJOIN metrics_metadata mm ON md.metric = mm.metric AND md.year = mm.period\nWHERE \n    md.metric = '${metric}'\n    AND md.metric_type IN ('Факт', 'План')\nGROUP BY time, md.metric, ml.month_name\nORDER BY time;",
